{
    "containerDefinitions": [
        {
            "name": "gm-backend",
            "image": "381491902283.dkr.ecr.us-east-2.amazonaws.com/gm-backend:v0.1",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "dependsOn": [
                {
                    "containerName": "redis",
                    "condition": "START"
                },
                {
                    "containerName": "dynamodb-local",
                    "condition": "START"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/gm-backend",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "redis",
            "image": "redis:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/gm-backend",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "gm-backend-task",
    "taskRoleArn": "arn:aws:iam::381491902283:role/task-admin",
    "executionRoleArn": "arn:aws:iam::381491902283:role/ECSTaskExecutionRole",
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048",
    "requiresCompatibilities": [
        "FARGATE"
    ]
}